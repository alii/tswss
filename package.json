{
	"name": "tswss",
	"packageManager": "yarn@4.2.2",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts",
		"release": "yarn build && yarn npm publish"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"dependencies": {
		"@types/ws": "^8.5.10",
		"alistair": "^1.9.0",
		"ws": "^8.17.0"
	},
	"devDependencies": {
		"tsup": "^8.0.2",
		"typescript": "^5.4.5"
	}
}
